{
  "name": "jadedui",
  "version": "0.1.3",
  "private": false,
  "description": "React Components Library",
  "author": "Jade",
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "React",
    "ant design",
    "typescript",
    "rollup"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/panyushan-jade/dui",
  "repository": {
    "type": "git",
    "url": "https://github.com/panyushan-jade/dui"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react-transition-group": "^4.4.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run clean && rollup -c",
    "eject": "react-scripts eject",
    "prepare": "npx husky install",
    "clean": "rimraf ./dist",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "node-sass --output-style compressed ./src/styles/index.scss ./dist/index.css",
    "lint:stylelint": "stylelint src/**/*.{css,scss} --fix --custom-syntax postcss-scss",
    "lint:eslint": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/src/*.{js,jsx,ts,tsx}": [
      "eslint --fix --ext .ts,.tsx"
    ],
    "**/*.{css,scss}": [
      "stylelint --fix --custom-syntax postcss-scss"
    ],
    "*.{js,jsx,,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "devDependencies": {
    "axios": "^1.1.3",
    "@storybook/addon-info": "^5.3.21",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "husky": "^7.0.4",
    "postcss": "^8.4.12",
    "sass": "^1.50.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "react-scripts": "5.0.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/node-logger": "^6.5.10",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@types/react-dom": "^18.0.2",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.44",
    "@types/react-transition-group": "^4.4.5",
    "@types/sass": "^1.43.1",
    "@types/storybook__addon-info": "^5.2.5",
    "autoprefixer": "^10.4.12",
    "babel-plugin-named-exports-order": "^0.0.2",
    "node-sass": "^7.0.3",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.3",
    "cssnano": "^5.1.13",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "lint-staged": "^12.3.8",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.6.2",
    "prop-types": "^15.8.1",
    "rollup": "^3.2.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "stylelint": "^14.6.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-scss": "^4.2.0",
    "webpack": "^5.74.0"
  },
  "peerDependencies":{
    "react": ">=^17.0.0",
    "react-dom": ">=^17.0.0",
    "axios": ">=^1.1.3"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
